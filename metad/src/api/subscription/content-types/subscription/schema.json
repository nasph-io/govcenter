{
  "kind": "collectionType",
  "collectionName": "subscriptions",
  "info": {
    "singularName": "subscription",
    "pluralName": "subscriptions",
    "displayName": "Subscription",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "clientid": {
      "type": "string"
    },
    "api_path": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "waiting_approval",
        "active",
        "expired",
        "blocked",
        "customer_blocked"
      ],
      "default": "waiting_approval"
    },
    "api_plan": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::api-plan.api-plan"
    },
    "currentRequests": {
      "type": "biginteger"
    },
    "comments": {
      "type": "text"
    },
    "totalRequests": {
      "type": "biginteger",
      "default": "0"
    },
    "invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice.invoice",
      "mappedBy": "subscription"
    },
    "portal_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::portal-user.portal-user",
      "inversedBy": "subscriptions"
    },
    "emails_subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::emails-subscription.emails-subscription",
      "mappedBy": "subscription"
    },
    "code_sei": {
      "type": "biginteger"
    },
    "code_saic": {
      "type": "biginteger"
    },
    "contract_term_start": {
      "type": "date"
    },
    "contract_term_end": {
      "type": "date"
    },
    "contract_term_additive": {
      "type": "date"
    }
  }
}
