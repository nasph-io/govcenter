    },
    "apiPlanId": {
      "type": "integer"
    },
    "uuid": {
      "type": "uid"
    },
    "subscription": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subscription.subscription",
      "inversedBy": "invoices"
